<button {{action "test"}}> Test</button>
 <div class="row col-md-12">
  <br>
   <div class="col-md-2 form-group" >
    <label>{{title}}</label>
     <select class="form-control" id="dayList">
      {{#each days as |day|}}
       <option value="{{day}}">{{day}}</option>
      {{/each}}
     </select>
   </div> 
 
   <div class="col-md-2 form-group">
    <label>&nbsp;</label>
     <select class="form-control" id="monthList">
      <option>1</option>
       <option>2</option>
        <option>3</option>
         <option>4</option>
          <option>5</option>
           <option>6</option>
            <option>7</option>
             <option>8</option>
              <option>9</option>
               <option>10</option>
                <option>11</option>
                 <option>12</option>
      </select>          
     </div> 
  
   
   
     <div class="col-md-3 form-group"> 
      <label>&nbsp;</label>
       <select id="year-list" class="form-control" id="yearList">
        {{#each years as |year|}}
         <option value="{{year}}">{{year}}</option>
        {{/each}} 
       </select>
     </div>
    
</div>
